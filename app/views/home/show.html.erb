<% if current_user %> 
  <h1><%= current_user.name %> meetings</h1>     
  <%= @calendars.count %>

  <ul>
    <% @calendars.each do |calendar| %>
      <li><strong><%= calendar.summary %></strong> </br>
      <%= calendar.start.date_time.strftime("%I:%M%p")%> to <%=calendar.end.date_time.strftime("%I:%M%p") %></li>
    <% end %>
  </ul>
<% else %>
      Nothing!
<% end %>